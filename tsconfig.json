{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "sourceMap": true,
      "baseUrl": ".",
      "paths": {
        "@/*": ["src/*"]
      },
      "resolveJsonModule": true,
      "lib": ["esnext", "dom"],
      "types": ["vite/client","node"]
    },
    "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue"]
  }
  